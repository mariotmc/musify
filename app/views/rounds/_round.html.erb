<%= turbo_stream_from "round_#{round.id}" %>

<% if round.started? %>
	<%= render partial: "rounds/started", locals: { round: round } %>
<% elsif round.ended? %>
	<%= render partial: "rounds/ended", locals: { round: round } %>
<% else %>
	<%= render partial: "rounds/waiting", locals: { round: round } %>
<% end %>
